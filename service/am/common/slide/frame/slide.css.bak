body{
margin:0
}

#slide_main {
float:left;
width:190px;
height:100%;
overflow:auto;
}
#slide_main form{
margin:5px 0 5px 5px;
width:100%;
}

#slide_arrow {
background:url("bg.png") repeat-y scroll 2px top transparent;
width:10px;
height:100%;
float:right;
}
#slide_arrow table{
width:100%;
height:100%;
}
#slide_separator{
width:1px;
}
